{
    "name": "eliberty/api-bundle",
    "type": "symfony-bundle",
    "description": "JSON-LD / Hydra REST API with fractal for Symfony",
    "keywords": ["api", "jsonld", "eliberty", "rest", "Hydra"],
    "license": "proprietary",
    "authors": [
        {
            "name": "philippe vesin",
            "email": "pvesin@eliberty.fr"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:eliberty/php-utils.git"
        },
        {
            "type": "git",
            "url": "git@github.com:eliberty/dunglas-api-core.git"
        }
    ],
    "require": {
        "php": ">=7.4",
        "nelmio/api-doc-bundle": "^2.0|^3.0",
        "league/fractal": "0.13.0",
        "eliberty/dunglas-api-core": "v1.2.3",
        "eliberty/php-utils": "0.2.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": { "Eliberty\\ApiBundle\\": "src/" }
    }
}
