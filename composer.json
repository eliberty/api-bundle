{
    "name": "eliberty/api-bundle",
    "type": "symfony-bundle",
    "description": "JSON-LD / Hydra REST API with fractal for Symfony",
    "keywords": ["api", "jsonld", "eliberty", "rest", "Hydra"],
    "license": "proprietary",
    "repositories": [
        {
            "type": "composer",
            "url": "https://e-liberty.repo.repman.rpstack.com"
        }
    ],
    "require": {
        "php": "^7.4 || 8.0",
        "nelmio/api-doc-bundle": "^2.0|^3.0",
        "league/fractal": "^0.13",
        "dunglas/api-bundle": "^1.2",
        "pagerfanta/pagerfanta": "^2.0|^3.0",
        "eliberty/php-utils": "0.2.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": { "Eliberty\\ApiBundle\\": "src/" }
    }
}
