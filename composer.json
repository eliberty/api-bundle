{
    "name": "eliberty/api-bundle",
    "type": "symfony-bundle",
    "description": "JSON-LD / Hydra REST API with fractal for Symfony",
    "keywords": ["api", "jsonld", "eliberty", "rest", "Hydra"],
    "license": "proprietary",
    "authors": [
        {
            "name": "philippe vesin",
            "email": "pvesin@eliberty.fr"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:eliberty/phplibs.git"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "nelmio/api-doc-bundle": "2.11.2",
        "league/fractal": "0.13.0",
        "dunglas/api-bundle": "v1.1.1",
        "white-october/pagerfanta-bundle": "~1.0.5",
        "eliberty/phplibs": "~3.0.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": { "Eliberty\\ApiBundle\\": "src/" }
    }
}
